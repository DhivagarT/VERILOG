S_0x5561c1bb2540 .scope module, "race_demo" "race_demo" 2 1;
 .timescale 0 0;
v0x5561c1bb2780_0 .var "a_block", 0 0;
v0x5561c1bfe110_0 .var "a_nonblock", 0 0;
v0x5561c1bfe1d0_0 .var "b_block", 0 0;
v0x5561c1bfe270_0 .var "b_nonblock", 0 0;
E_0x5561c1bb2c60 .event anyedge, v0x5561c1bfe110_0;
E_0x5561c1bb2a70 .event anyedge, v0x5561c1bfe270_0;
E_0x5561c1bb2dc0 .event anyedge, v0x5561c1bb2780_0;
E_0x5561c1bd61d0 .event anyedge, v0x5561c1bfe1d0_0;
    .scope S_0x5561c1bb2540;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5561c1bb2780_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5561c1bfe1d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5561c1bfe110_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5561c1bfe270_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5561c1bb2540;
T_1 ;
    %wait E_0x5561c1bd61d0;
    %delay 1, 0;
    %load/vec4 v0x5561c1bfe1d0_0;
    %store/vec4 v0x5561c1bb2780_0, 0, 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5561c1bb2540;
T_2 ;
    %wait E_0x5561c1bb2dc0;
    %delay 1, 0;
    %load/vec4 v0x5561c1bb2780_0;
    %store/vec4 v0x5561c1bfe1d0_0, 0, 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x5561c1bb2540;
T_3 ;
    %wait E_0x5561c1bb2a70;
    %delay 1, 0;
    %load/vec4 v0x5561c1bfe270_0;
    %assign/vec4 v0x5561c1bfe110_0, 0;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x5561c1bb2540;
T_4 ;
    %wait E_0x5561c1bb2c60;
    %delay 1, 0;
    %load/vec4 v0x5561c1bfe110_0;
    %assign/vec4 v0x5561c1bfe270_0, 0;
    %jmp T_4;
    .thread T_4, $push;
    .scope S_0x5561c1bb2540;
T_5 ;
    %vpi_call 2 29 "$display", "Time a_block b_block a_nonblock b_nonblock" {0 0 0};
    %vpi_call 2 30 "$monitor", "%0t %0d %0d %0d %0d", $time, v0x5561c1bb2780_0, v0x5561c1bfe1d0_0, v0x5561c1bfe110_0, v0x5561c1bfe270_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
