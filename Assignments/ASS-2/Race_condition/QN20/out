 .timescale 0 0;
v0x5b3abe535740_0 .var "a", 0 0;
v0x5b3abe57e430_0 .var "b", 0 0;
E_0x5b3abe535910 .event anyedge, v0x5b3abe535740_0;
E_0x5b3abe535a90 .event anyedge, v0x5b3abe57e430_0;
    .scope S_0x5b3abe535570;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b3abe535740_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b3abe57e430_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5b3abe535570;
T_1 ;
    %wait E_0x5b3abe535a90;
    %delay 1, 0;
    %load/vec4 v0x5b3abe57e430_0;
    %assign/vec4 v0x5b3abe535740_0, 0;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5b3abe535570;
T_2 ;
    %wait E_0x5b3abe535910;
    %delay 1, 0;
    %load/vec4 v0x5b3abe535740_0;
    %assign/vec4 v0x5b3abe57e430_0, 0;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x5b3abe535570;
T_3 ;
    %vpi_call 2 21 "$monitor", "time=%0t | a=%0d b=%0d", $time, v0x5b3abe535740_0, v0x5b3abe57e430_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_3;
