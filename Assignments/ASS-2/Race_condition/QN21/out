#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61eebfd2b570 .scope module, "non_blocking_demo" "non_blocking_demo" 2 1;
 .timescale 0 0;
v0x61eebfd2b720_0 .var "a", 0 0;
v0x61eebfd73b80_0 .var "b", 0 0;
E_0x61eebfd2ba30 .event anyedge, v0x61eebfd2b720_0, v0x61eebfd73b80_0;
    .scope S_0x61eebfd2b570;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61eebfd2b720_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61eebfd73b80_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x61eebfd2b570;
T_1 ;
    %wait E_0x61eebfd2ba30;
    %load/vec4 v0x61eebfd2b720_0;
    %assign/vec4 v0x61eebfd73b80_0, 0;
    %load/vec4 v0x61eebfd73b80_0;
    %assign/vec4 v0x61eebfd2b720_0, 0;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x61eebfd2b570;
T_2 ;
    %vpi_call 2 15 "$monitor", "Time=%0t | a=%0d b=%0d", $time, v0x61eebfd2b720_0, v0x61eebfd73b80_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN21.v";
