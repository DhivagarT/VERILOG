#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55da99bd5510 .scope module, "non_blocking" "non_blocking" 2 1;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "a";
    .port_info 1 /OUTPUT 1 "b";
v0x55da99bd56a0_0 .var "a", 0 0;
v0x55da99c1e5d0_0 .var "b", 0 0;
v0x55da99c1e690_0 .var "x", 0 0;
v0x55da99c1e730_0 .var "y", 0 0;
E_0x55da99bd5e20 .event anyedge, v0x55da99c1e690_0, v0x55da99c1e730_0;
    .scope S_0x55da99bd5510;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55da99c1e690_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55da99c1e730_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55da99bd5510;
T_1 ;
    %wait E_0x55da99bd5e20;
    %load/vec4 v0x55da99c1e690_0;
    %load/vec4 v0x55da99c1e730_0;
    %or;
    %assign/vec4 v0x55da99bd56a0_0, 0;
    %load/vec4 v0x55da99c1e690_0;
    %load/vec4 v0x55da99c1e730_0;
    %xor;
    %assign/vec4 v0x55da99c1e5d0_0, 0;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x55da99bd5510;
T_2 ;
    %vpi_call 2 16 "$monitor", "Time=%0t a=%0d b=%0d", $time, v0x55da99bd56a0_0, v0x55da99c1e5d0_0 {0 0 0};
    %delay 30, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN2.v";
