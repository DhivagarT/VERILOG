#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x594a6864e560 .scope module, "mix_example2" "mix_example2" 2 1;
 .timescale 0 0;
v0x594a68650400_0 .var "x", 0 0;
v0x594a68695d50_0 .var "y", 0 0;
v0x594a68695e10_0 .var "z", 0 0;
    .scope S_0x594a6864e560;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x594a68650400_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594a68695d50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x594a68695e10_0, 0, 1;
    %load/vec4 v0x594a68650400_0;
    %load/vec4 v0x594a68695d50_0;
    %add;
    %assign/vec4 v0x594a68695e10_0, 0;
    %load/vec4 v0x594a68650400_0;
    %load/vec4 v0x594a68695e10_0;
    %add;
    %store/vec4 v0x594a68695d50_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 13 "$display", "x=%0d y=%0d z=%0d", v0x594a68650400_0, v0x594a68695d50_0, v0x594a68695e10_0 {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
