#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6475049cd510 .scope module, "non_blocking_demo" "non_blocking_demo" 2 1;
 .timescale 0 0;
v0x6475049cd6c0_0 .var "A", 3 0;
v0x647504a16c70_0 .var "B", 3 0;
v0x647504a16d50_0 .var "clk", 0 0;
E_0x6475049cdc30 .event posedge, v0x647504a16d50_0;
    .scope S_0x6475049cd510;
T_0 ;
    %wait E_0x6475049cdc30;
    %load/vec4 v0x647504a16c70_0;
    %assign/vec4 v0x6475049cd6c0_0, 0;
    %load/vec4 v0x6475049cd6c0_0;
    %assign/vec4 v0x647504a16c70_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6475049cd510;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x647504a16d50_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x6475049cd510;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x647504a16d50_0;
    %inv;
    %store/vec4 v0x647504a16d50_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x6475049cd510;
T_3 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x6475049cd6c0_0, 0, 4;
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x647504a16c70_0, 0, 4;
    %delay 50, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x6475049cd510;
T_4 ;
    %vpi_call 2 20 "$monitor", "Time=%0t clk=%0d A=%0d B=%0d", $time, v0x647504a16d50_0, v0x6475049cd6c0_0, v0x647504a16c70_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN16.v";
