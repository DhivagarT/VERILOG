:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5935c3f8b5e0 .scope module, "tb_mux4_if" "tb_mux4_if" 2 19;
 .timescale 0 0;
v0x5935c3f9e520_0 .var "a", 3 0;
v0x5935c3f9e610_0 .var "sel", 1 0;
v0x5935c3f9e6e0_0 .net "y", 0 0, v0x5935c3f9e400_0;  1 drivers
S_0x5935c3f8b770 .scope module, "uut" "mux4_if" 2 24, 2 1 0, S_0x5935c3f8b5e0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 2 "sel";
    .port_info 2 /OUTPUT 1 "y";
v0x5935c3f8b9a0_0 .net "a", 3 0, v0x5935c3f9e520_0;  1 drivers
v0x5935c3f9e320_0 .net "sel", 1 0, v0x5935c3f9e610_0;  1 drivers
v0x5935c3f9e400_0 .var "y", 0 0;
E_0x5935c3f51a70 .event anyedge, v0x5935c3f9e320_0, v0x5935c3f8b9a0_0;
    .scope S_0x5935c3f8b770;
T_0 ;
    %wait E_0x5935c3f51a70;
    %load/vec4 v0x5935c3f9e320_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x5935c3f8b9a0_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x5935c3f9e400_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5935c3f9e320_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x5935c3f8b9a0_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x5935c3f9e400_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5935c3f9e320_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x5935c3f8b9a0_0;
    %parti/s 1, 2, 3;
    %store/vec4 v0x5935c3f9e400_0, 0, 1;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5935c3f8b9a0_0;
    %parti/s 1, 3, 3;
    %store/vec4 v0x5935c3f9e400_0, 0, 1;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5935c3f8b5e0;
T_1 ;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5935c3f9e520_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5935c3f9e610_0, 0, 2;
    %pushi/vec4 8, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 2, 0;
    %load/vec4 v0x5935c3f9e610_0;
    %addi 1, 0, 2;
    %store/vec4 v0x5935c3f9e610_0, 0, 2;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %end;
    .thread T_1;
    .scope S_0x5935c3f8b5e0;
T_2 ;
    %vpi_call 2 36 "$monitor", "Time=%0t | a=%b sel=%b y=%b", $time, v0x5935c3f9e520_0, v0x5935c3f9e610_0, v0x5935c3f9e6e0_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 37 "$finish" {0 0 0};
    %end;
    .thread T_2;
