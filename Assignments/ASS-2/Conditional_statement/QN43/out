:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d459eb76760 .scope module, "tb_if_logic" "tb_if_logic" 2 14;
 .timescale 0 0;
v0x5d459eb89100_0 .var "a", 0 0;
v0x5d459eb891d0_0 .var "b", 0 0;
v0x5d459eb892a0_0 .net "y", 0 0, v0x5d459eb88fe0_0;  1 drivers
S_0x5d459eb768f0 .scope module, "uut" "if_logic" 2 18, 2 1 0, S_0x5d459eb76760;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
v0x5d459eb76b40_0 .net "a", 0 0, v0x5d459eb89100_0;  1 drivers
v0x5d459eb88f20_0 .net "b", 0 0, v0x5d459eb891d0_0;  1 drivers
v0x5d459eb88fe0_0 .var "y", 0 0;
E_0x5d459eb3e6a0 .event anyedge, v0x5d459eb76b40_0, v0x5d459eb88f20_0;
    .scope S_0x5d459eb768f0;
T_0 ;
    %wait E_0x5d459eb3e6a0;
    %load/vec4 v0x5d459eb76b40_0;
    %load/vec4 v0x5d459eb88f20_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d459eb88fe0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d459eb88fe0_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5d459eb76760;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d459eb89100_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d459eb891d0_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d459eb89100_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d459eb891d0_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d459eb89100_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d459eb891d0_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d459eb89100_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d459eb891d0_0, 0, 1;
    %delay 2, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5d459eb76760;
T_2 ;
    %vpi_call 2 29 "$monitor", "Time=%0t | a=%b b=%b | y=%b", $time, v0x5d459eb89100_0, v0x5d459eb891d0_0, v0x5d459eb892a0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
