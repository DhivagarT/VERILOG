:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6182e9bf9900 .scope module, "tb_mux_if" "tb_mux_if" 2 14;
 .timescale 0 0;
v0x6182e9c0bb50_0 .var "a", 0 0;
v0x6182e9c0bc10_0 .var "b", 0 0;
v0x6182e9c0bce0_0 .var "sel", 0 0;
v0x6182e9c0bde0_0 .net "y", 0 0, v0x6182e9c0b9e0_0;  1 drivers
S_0x6182e9bf9a90 .scope module, "uut" "mux_if" 2 18, 2 1 0, S_0x6182e9bf9900;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /INPUT 1 "sel";
    .port_info 3 /OUTPUT 1 "y";
v0x6182e9bc0490_0 .net "a", 0 0, v0x6182e9c0bb50_0;  1 drivers
v0x6182e9c0b850_0 .net "b", 0 0, v0x6182e9c0bc10_0;  1 drivers
v0x6182e9c0b910_0 .net "sel", 0 0, v0x6182e9c0bce0_0;  1 drivers
v0x6182e9c0b9e0_0 .var "y", 0 0;
E_0x6182e9bc0b60 .event anyedge, v0x6182e9c0b910_0, v0x6182e9c0b850_0, v0x6182e9bc0490_0;
    .scope S_0x6182e9bf9a90;
T_0 ;
    %wait E_0x6182e9bc0b60;
    %load/vec4 v0x6182e9c0b910_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x6182e9c0b850_0;
    %store/vec4 v0x6182e9c0b9e0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6182e9bc0490_0;
    %store/vec4 v0x6182e9c0b9e0_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x6182e9bf9900;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6182e9c0bb50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6182e9c0bc10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6182e9c0bce0_0, 0, 1;
T_1.0 ;
    %delay 2, 0;
    %load/vec4 v0x6182e9c0bce0_0;
    %inv;
    %store/vec4 v0x6182e9c0bce0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x6182e9bf9900;
T_2 ;
    %vpi_call 2 28 "$monitor", "Time=%0t | a=%0d b=%0d sel=%0d y=%0d", $time, v0x6182e9c0bb50_0, v0x6182e9c0bc10_0, v0x6182e9c0bce0_0, v0x6182e9c0bde0_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
