:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c1c32fee300 .scope module, "tb_encoder_if" "tb_encoder_if" 2 20;
 .timescale 0 0;
v0x5c1c33000c30_0 .var "in", 3 0;
v0x5c1c33000cf0_0 .net "out", 1 0, v0x5c1c33000af0_0;  1 drivers
S_0x5c1c32fee490 .scope module, "uut" "encoder_if" 2 24, 2 1 0, S_0x5c1c32fee300;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in";
    .port_info 1 /OUTPUT 2 "out";
v0x5c1c32fee670_0 .net "in", 3 0, v0x5c1c33000c30_0;  1 drivers
v0x5c1c33000af0_0 .var "out", 1 0;
E_0x5c1c32fb4a70 .event anyedge, v0x5c1c32fee670_0;
    .scope S_0x5c1c32fee490;
T_0 ;
    %wait E_0x5c1c32fb4a70;
    %load/vec4 v0x5c1c32fee670_0;
    %parti/s 1, 3, 3;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x5c1c33000af0_0, 0, 2;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5c1c32fee670_0;
    %parti/s 1, 2, 3;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x5c1c33000af0_0, 0, 2;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5c1c32fee670_0;
    %parti/s 1, 1, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x5c1c33000af0_0, 0, 2;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5c1c32fee670_0;
    %parti/s 1, 0, 2;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5c1c33000af0_0, 0, 2;
    %jmp T_0.7;
T_0.6 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5c1c33000af0_0, 0, 2;
T_0.7 ;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5c1c32fee300;
T_1 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5c1c33000c30_0, 0, 4;
    %delay 2, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5c1c33000c30_0, 0, 4;
    %delay 2, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x5c1c33000c30_0, 0, 4;
    %delay 2, 0;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5c1c33000c30_0, 0, 4;
    %delay 2, 0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5c1c33000c30_0, 0, 4;
    %delay 2, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5c1c32fee300;
T_2 ;
    %vpi_call 2 36 "$monitor", "Time=%0t | in=%b | out=%b", $time, v0x5c1c33000c30_0, v0x5c1c33000cf0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
