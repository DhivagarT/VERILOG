#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56cb7687ea00 .scope module, "tb_sync_counter" "tb_sync_counter" 2 16;
 .timescale 0 0;
v0x56cb76891680_0 .var "clk", 0 0;
v0x56cb76891750_0 .net "count", 3 0, v0x56cb76891480_0;  1 drivers
v0x56cb76891820_0 .var "reset", 0 0;
S_0x56cb7687eb90 .scope module, "uut" "sync_counter" 2 20, 2 1 0, S_0x56cb7687ea00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x56cb7687ede0_0 .net "clk", 0 0, v0x56cb76891680_0;  1 drivers
v0x56cb76891480_0 .var "count", 3 0;
v0x56cb76891560_0 .net "reset", 0 0, v0x56cb76891820_0;  1 drivers
E_0x56cb768466a0 .event posedge, v0x56cb7687ede0_0;
    .scope S_0x56cb7687eb90;
T_0 ;
    %wait E_0x56cb768466a0;
    %load/vec4 v0x56cb76891560_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x56cb76891480_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x56cb76891480_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x56cb76891480_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x56cb7687ea00;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56cb76891680_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x56cb76891680_0;
    %inv;
    %store/vec4 v0x56cb76891680_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x56cb7687ea00;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56cb76891820_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56cb76891820_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56cb76891820_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56cb76891820_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x56cb7687ea00;
T_3 ;
    %vpi_call 2 35 "$monitor", "Time=%0t | reset=%0d | count=%b", $time, v0x56cb76891820_0, v0x56cb76891750_0 {0 0 0};
    %delay 150, 0;
    %vpi_call 2 36 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
