T_0.9 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55b52fb05540;
T_1 ;
    %vpi_call 2 21 "$monitor", "Time=%0t addr=%0d data=%0d", $time, v0x55b52fb056f0_0, v0x55b52fb509b0_0 {0 0 0};
