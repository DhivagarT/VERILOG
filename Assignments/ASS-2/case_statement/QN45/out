#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6366cb2b5510 .scope module, "four_one_mux" "four_one_mux" 2 1;
 .timescale 0 0;
v0x6366cb2b56c0_0 .var "a", 3 0;
v0x6366cb3004f0_0 .var "sel", 1 0;
v0x6366cb3005d0_0 .var "y", 0 0;
E_0x6366cb2b5f40 .event anyedge, v0x6366cb3004f0_0, v0x6366cb2b56c0_0;
    .scope S_0x6366cb2b5510;
T_0 ;
    %wait E_0x6366cb2b5f40;
    %load/vec4 v0x6366cb3004f0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6366cb3005d0_0, 0, 1;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x6366cb2b56c0_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x6366cb3005d0_0, 0, 1;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x6366cb2b56c0_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x6366cb3005d0_0, 0, 1;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x6366cb2b56c0_0;
    %parti/s 1, 2, 3;
    %store/vec4 v0x6366cb3005d0_0, 0, 1;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x6366cb2b56c0_0;
    %parti/s 1, 3, 3;
    %store/vec4 v0x6366cb3005d0_0, 0, 1;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x6366cb2b5510;
T_1 ;
    %vpi_call 2 18 "$monitor", "Time=%0t a=%b sel=%b y=%b", $time, v0x6366cb2b56c0_0, v0x6366cb3004f0_0, v0x6366cb3005d0_0 {0 0 0};
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x6366cb2b56c0_0, 0, 4;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x6366cb3004f0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x6366cb3004f0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x6366cb3004f0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x6366cb3004f0_0, 0, 2;
    %delay 5, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN1.v";
