#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5784bf57b570 .scope module, "multiblock" "multiblock" 2 1;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "a";
    .port_info 1 /OUTPUT 1 "b";
v0x5784bf57b700_0 .var "a", 0 0;
v0x5784bf5c41b0_0 .var "b", 0 0;
    .scope S_0x5784bf57b570;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5784bf57b700_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5784bf5c41b0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5784bf57b570;
T_1 ;
    %delay 2, 0;
    %load/vec4 v0x5784bf57b700_0;
    %inv;
    %assign/vec4 v0x5784bf57b700_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5784bf57b570;
T_2 ;
    %delay 3, 0;
    %load/vec4 v0x5784bf5c41b0_0;
    %inv;
    %assign/vec4 v0x5784bf5c41b0_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5784bf57b570;
T_3 ;
    %vpi_call 2 16 "$monitor", "Time=%0t a=%0d b=%0d ", $time, v0x5784bf57b700_0, v0x5784bf5c41b0_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN3.v";
