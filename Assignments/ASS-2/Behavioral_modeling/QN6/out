#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56b1f6cc7570 .scope module, "setup_functional_update" "setup_functional_update" 2 1;
 .timescale 0 0;
    .port_info 0 /OUTPUT 4 "count";
v0x56b1f6cc7700_0 .var "count", 3 0;
    .scope S_0x56b1f6cc7570;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x56b1f6cc7700_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x56b1f6cc7570;
T_1 ;
    %delay 3, 0;
    %load/vec4 v0x56b1f6cc7700_0;
    %inv;
    %store/vec4 v0x56b1f6cc7700_0, 0, 4;
    %jmp T_1;
    .thread T_1;
    .scope S_0x56b1f6cc7570;
T_2 ;
    %vpi_call 2 11 "$monitor", "Time=%0t count=%0d", $time, v0x56b1f6cc7700_0 {0 0 0};
    %delay 30, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN6.v";
