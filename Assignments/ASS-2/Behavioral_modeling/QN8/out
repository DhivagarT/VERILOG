#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x601f7e1c3570 .scope module, "two_always" "two_always" 2 1;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "x";
    .port_info 1 /OUTPUT 1 "y";
v0x601f7e1c3700_0 .var "x", 0 0;
v0x601f7e20bb20_0 .var "y", 0 0;
    .scope S_0x601f7e1c3570;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x601f7e1c3700_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x601f7e20bb20_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x601f7e1c3570;
T_1 ;
    %delay 2, 0;
    %load/vec4 v0x601f7e1c3700_0;
    %inv;
    %store/vec4 v0x601f7e1c3700_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x601f7e1c3570;
T_2 ;
    %delay 4, 0;
    %load/vec4 v0x601f7e20bb20_0;
    %inv;
    %store/vec4 v0x601f7e20bb20_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x601f7e1c3570;
T_3 ;
    %vpi_call 2 11 "$monitor", "Time=%0t x=%0d y=%0d ", $time, v0x601f7e1c3700_0, v0x601f7e20bb20_0 {0 0 0};
    %delay 40, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN8.v";
