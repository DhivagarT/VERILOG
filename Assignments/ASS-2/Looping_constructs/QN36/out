:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5830b52424c0 .scope module, "forever_example" "forever_example" 2 1;
 .timescale 0 0;
v0x5830b5267b90_0 .var/i "count", 31 0;
v0x5830b5267cc0_0 .var "internal_clk", 0 0;
v0x5830b528eef0_0 .var "q1", 0 0;
v0x5830b528ef90_0 .var "q2", 0 0;
v0x5830b528f050_0 .var "reset", 0 0;
E_0x5830b5242ae0 .event posedge, v0x5830b528f050_0, v0x5830b5267cc0_0;
    .scope S_0x5830b52424c0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5830b5267cc0_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v0x5830b5267cc0_0;
    %inv;
    %store/vec4 v0x5830b5267cc0_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x5830b52424c0;
T_1 ;
    %wait E_0x5830b5242ae0;
    %load/vec4 v0x5830b528f050_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5830b528eef0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5830b528eef0_0;
    %inv;
    %assign/vec4 v0x5830b528eef0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5830b52424c0;
T_2 ;
    %wait E_0x5830b5242ae0;
    %load/vec4 v0x5830b528f050_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x5830b5267b90_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5830b528ef90_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x5830b5267b90_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz  T_2.2, 5;
    %load/vec4 v0x5830b528ef90_0;
    %inv;
    %assign/vec4 v0x5830b528ef90_0, 0;
    %load/vec4 v0x5830b5267b90_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x5830b5267b90_0, 0;
T_2.2 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5830b52424c0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5830b528f050_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5830b528f050_0, 0, 1;
    %delay 60, 0;
    %vpi_call 2 34 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5830b52424c0;
T_4 ;
    %vpi_call 2 38 "$monitor", "Time=%0t | internal_clk=%b | q1=%b | q2=%b | count=%0d", $time, v0x5830b5267cc0_0, v0x5830b528eef0_0, v0x5830b528ef90_0, v0x5830b5267b90_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
