#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5be0ac4f3540 .scope module, "memory_init" "memory_init" 2 1;
 .timescale 0 0;
v0x5be0ac4f36d0_0 .var/i "i", 31 0;
v0x5be0ac53ba00 .array "mem", 15 0, 7 0;
    .scope S_0x5be0ac4f3540;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5be0ac4f36d0_0, 0, 32;
    %pushi/vec4 16, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %load/vec4 v0x5be0ac4f36d0_0;
    %pad/s 8;
    %ix/getv/s 4, v0x5be0ac4f36d0_0;
    %store/vec4a v0x5be0ac53ba00, 4, 0;
    %load/vec4 v0x5be0ac4f36d0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5be0ac4f36d0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5be0ac4f36d0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x5be0ac4f36d0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.3, 5;
    %vpi_call 2 13 "$display", "mem[%0d] = %0d", v0x5be0ac4f36d0_0, &A<v0x5be0ac53ba00, v0x5be0ac4f36d0_0 > {0 0 0};
    %load/vec4 v0x5be0ac4f36d0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5be0ac4f36d0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
