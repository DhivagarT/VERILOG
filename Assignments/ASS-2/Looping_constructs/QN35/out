S_0x650e63f55540 .scope module, "loop_waveform" "loop_waveform" 2 1;
 .timescale 0 0;
v0x650e63f556d0_0 .var "clk_for", 0 0;
v0x650e63f9f830_0 .var "clk_forever", 0 0;
v0x650e63f9f8f0_0 .var "clk_repeat", 0 0;
v0x650e63f9f990_0 .var/i "i", 31 0;
    .scope S_0x650e63f55540;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650e63f556d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x650e63f9f990_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x650e63f9f990_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_0.1, 5;
    %delay 5, 0;
    %load/vec4 v0x650e63f556d0_0;
    %inv;
    %store/vec4 v0x650e63f556d0_0, 0, 1;
    %load/vec4 v0x650e63f9f990_0;
    %addi 1, 0, 32;
    %store/vec4 v0x650e63f9f990_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x650e63f55540;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650e63f9f8f0_0, 0, 1;
    %pushi/vec4 5, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %load/vec4 v0x650e63f9f8f0_0;
    %inv;
    %store/vec4 v0x650e63f9f8f0_0, 0, 1;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %end;
    .thread T_1;
    .scope S_0x650e63f55540;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x650e63f9f830_0, 0, 1;
T_2.0 ;
    %delay 3, 0;
    %load/vec4 v0x650e63f9f830_0;
    %inv;
    %store/vec4 v0x650e63f9f830_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x650e63f55540;
T_3 ;
    %vpi_call 2 28 "$dumpfile", "loop_waveform.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x650e63f55540 {0 0 0};
    %delay 100, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
