#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x648b00f2a540 .scope module, "forever_disable" "forever_disable" 2 1;
 .timescale 0 0;
v0x648b00f2a6d0_0 .var "clk", 0 0;
v0x648b00f735f0_0 .var/i "count", 31 0;
    .scope S_0x648b00f2a540;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x648b00f2a6d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x648b00f735f0_0, 0, 32;
T_0.0 ;
    %delay 2, 0;
    %load/vec4 v0x648b00f2a6d0_0;
    %inv;
    %store/vec4 v0x648b00f2a6d0_0, 0, 1;
    %load/vec4 v0x648b00f735f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x648b00f735f0_0, 0, 32;
    %vpi_call 2 12 "$display", "Time=%0t | clk=%b | count=%0d", $time, v0x648b00f2a6d0_0, v0x648b00f735f0_0 {0 0 0};
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x648b00f2a540;
T_1 ;
    %vpi_call 2 17 "$display", "Forever loop disabled at time %0t", $time {0 0 0};
    %delay 2, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN31.v";
