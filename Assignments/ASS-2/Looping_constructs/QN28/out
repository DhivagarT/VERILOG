:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x557f4bffa530 .scope module, "counter" "counter" 2 1;
 .timescale 0 0;
v0x557f4bffa6c0_0 .var/i "count", 31 0;
v0x557f4c043110_0 .var "data", 3 0;
v0x557f4c0431f0_0 .var/i "i", 31 0;
    .scope S_0x557f4bffa530;
T_0 ;
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x557f4c043110_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x557f4c0431f0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x557f4bffa6c0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x557f4c0431f0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x557f4c0431f0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x557f4c043110_0;
    %load/vec4 v0x557f4c0431f0_0;
    %part/s 1;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x557f4bffa6c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x557f4bffa6c0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x557f4c0431f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x557f4c0431f0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 16 "$display", "Data = %b, Number of 1s = %0d", v0x557f4c043110_0, v0x557f4bffa6c0_0 {0 0 0};
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
