#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59f492bf5570 .scope module, "counter_repeat" "counter_repeat" 2 1;
 .timescale 0 0;
v0x59f492bf5720_0 .var "clk", 0 0;
v0x59f492c3df90_0 .var "count", 2 0;
E_0x59f492bf66c0 .event posedge, v0x59f492bf5720_0;
    .scope S_0x59f492bf5570;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x59f492bf5720_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v0x59f492bf5720_0;
    %inv;
    %store/vec4 v0x59f492bf5720_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x59f492bf5570;
T_1 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x59f492c3df90_0, 0, 3;
    %pushi/vec4 8, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x59f492bf66c0;
    %load/vec4 v0x59f492c3df90_0;
    %addi 1, 0, 3;
    %store/vec4 v0x59f492c3df90_0, 0, 3;
    %vpi_call 2 15 "$display", "Time=%0t | count=%0d", $time, v0x59f492c3df90_0 {0 0 0};
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
