T_0.0 ;
    %load/vec4 v0x5db6a9538ee0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5db6a9538e00_0;
    %load/vec4 v0x5db6a9538ee0_0;
    %part/s 1;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x5db6a94f06c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5db6a94f06c0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x5db6a9538ee0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5db6a9538ee0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
