    .scope S_0x5db6a94f0530;
T_0 ;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x5db6a9538e00_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5db6a9538ee0_0, 0, 32;
