#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ec5c78b3540 .scope module, "odd_prority_xor" "odd_prority_xor" 2 1;
 .timescale 0 0;
v0x5ec5c78fd8f0_0 .var "a", 3 0;
v0x5ec5c78fd9f0_0 .var "y", 0 0;
E_0x5ec5c78b39f0 .event anyedge, v0x5ec5c78fd8f0_0;
S_0x5ec5c78b36f0 .scope function.vec4.s1, "odd_finding" "odd_finding" 2 6, 2 6 0, S_0x5ec5c78b3540;
 .timescale 0 0;
v0x5ec5c78dabd0_0 .var "a", 3 0;
; Variable odd_finding is vec4 return value of scope S_0x5ec5c78b36f0
TD_odd_prority_xor.odd_finding ;
    %load/vec4 v0x5ec5c78dabd0_0;
    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %mod;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 1;
    %ret/vec4 0, 0, 1;  Assign to odd_finding (store_vec4_to_lval)
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5ec5c78dabd0_0;
    %xor/r;
    %ret/vec4 0, 0, 1;  Assign to odd_finding (store_vec4_to_lval)
T_0.1 ;
    %end;
    .scope S_0x5ec5c78b3540;
T_1 ;
    %delay 3, 0;
    %load/vec4 v0x5ec5c78fd8f0_0;
    %inv;
    %store/vec4 v0x5ec5c78fd8f0_0, 0, 4;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5ec5c78b3540;
T_2 ;
    %wait E_0x5ec5c78b39f0;
    %load/vec4 v0x5ec5c78fd8f0_0;
    %store/vec4 v0x5ec5c78dabd0_0, 0, 4;
    %callf/vec4 TD_odd_prority_xor.odd_finding, S_0x5ec5c78b36f0;
    %store/vec4 v0x5ec5c78fd9f0_0, 0, 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x5ec5c78b3540;
T_3 ;
    %vpi_func 2 21 "$urandom" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x5ec5c78fd8f0_0, 0, 4;
    %vpi_call 2 22 "$monitor", "time=%0t a=%0d y=%0d", $time, v0x5ec5c78fd8f0_0, v0x5ec5c78fd9f0_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "odd_priority_xor.v";
