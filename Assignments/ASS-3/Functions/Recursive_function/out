#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e1ac0a5d560 .scope module, "factorial_function" "factorial_function" 2 1;
 .timescale 0 0;
v0x5e1ac0aa6870_0 .var/i "num", 31 0;
S_0x5e1ac0a5d6f0 .scope function.vec4.u32, "factorial" "factorial" 2 5, 2 5 0, S_0x5e1ac0a5d560;
 .timescale 0 0;
; Variable factorial is vec4 return value of scope S_0x5e1ac0a5d6f0
v0x5e1ac0aa66d0_0 .var/i "i", 31 0;
v0x5e1ac0aa67b0_0 .var/i "n", 31 0;
TD_factorial_function.factorial ;
    %pushi/vec4 1, 0, 32;
    %ret/vec4 0, 0, 32;  Assign to factorial (store_vec4_to_lval)
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x5e1ac0aa66d0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5e1ac0aa66d0_0;
    %load/vec4 v0x5e1ac0aa67b0_0;
    %cmp/s;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %retload/vec4 0; Load factorial (draw_signal_vec4)
    %load/vec4 v0x5e1ac0aa66d0_0;
    %mul;
    %ret/vec4 0, 0, 32;  Assign to factorial (store_vec4_to_lval)
    %load/vec4 v0x5e1ac0aa66d0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5e1ac0aa66d0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x5e1ac0a5d560;
T_1 ;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x5e1ac0aa6870_0, 0, 32;
    %load/vec4 v0x5e1ac0aa6870_0;
    %store/vec4 v0x5e1ac0aa67b0_0, 0, 32;
    %callf/vec4 TD_factorial_function.factorial, S_0x5e1ac0a5d6f0;
    %vpi_call 2 17 "$display", "Factorial of %0d = %0d", v0x5e1ac0aa6870_0, S<0,vec4,s32> {1 0 0};
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
