    %load/vec4 v0x5cde92cd0fe0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5cde92cd0fe0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x5cde92c87560;
T_1 ;
    %pushi/vec4 247, 0, 8;
    %store/vec4 v0x5cde92cd1180_0, 0, 8;
