TD_and_all_bits.and_all ;
    %pushi/vec4 1, 0, 1;
    %ret/vec4 0, 0, 1;  Assign to and_all (store_vec4_to_lval)
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5cde92cd0fe0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5cde92cd0fe0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %retload/vec4 0; Load and_all (draw_signal_vec4)
    %load/vec4 v0x5cde92cd10c0_0;
    %load/vec4 v0x5cde92cd0fe0_0;
    %part/s 1;
    %and;
