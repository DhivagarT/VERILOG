:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x628f1f056560 .scope module, "shift_right_n" "shift_right_n" 2 1;
 .timescale 0 0;
v0x628f1f0a0120_0 .var "a", 7 0;
v0x628f1f0a0220_0 .var "n", 3 0;
v0x628f1f0a0300_0 .var "r", 7 0;
S_0x628f1f0566f0 .scope function.vec4.s8, "sh_right" "sh_right" 2 6, 2 6 0, S_0x628f1f056560;
 .timescale 0 0;
v0x628f1f07cf50_0 .var "sh", 3 0;
; Variable sh_right is vec4 return value of scope S_0x628f1f0566f0
v0x628f1f0a0060_0 .var "x", 7 0;
TD_shift_right_n.sh_right ;
    %load/vec4 v0x628f1f0a0060_0;
    %ix/getv 4, v0x628f1f07cf50_0;
    %shiftr 4;
    %ret/vec4 0, 0, 8;  Assign to sh_right (store_vec4_to_lval)
    %end;
    .scope S_0x628f1f056560;
T_1 ;
    %pushi/vec4 216, 0, 8;
    %store/vec4 v0x628f1f0a0120_0, 0, 8;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x628f1f0a0220_0, 0, 4;
    %load/vec4 v0x628f1f0a0120_0;
    %load/vec4 v0x628f1f0a0220_0;
    %store/vec4 v0x628f1f07cf50_0, 0, 4;
    %store/vec4 v0x628f1f0a0060_0, 0, 8;
    %callf/vec4 TD_shift_right_n.sh_right, S_0x628f1f0566f0;
    %store/vec4 v0x628f1f0a0300_0, 0, 8;
    %vpi_call 2 18 "$display", "a=%b n=%0d shifted=%b", v0x628f1f0a0120_0, v0x628f1f0a0220_0, v0x628f1f0a0300_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
