#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x565461cbe510 .scope module, "sum_of_twobit" "sum_of_twobit" 2 1;
 .timescale 0 0;
v0x565461d09450_0 .var "a", 7 0;
v0x565461d09550_0 .var "b", 7 0;
v0x565461d09630_0 .var "c", 7 0;
E_0x565461cbec20 .event anyedge, v0x565461d09450_0, v0x565461d09550_0;
S_0x565461cbe6c0 .scope function.vec4.s8, "sum" "sum" 2 4, 2 4 0, S_0x565461cbe510;
 .timescale 0 0;
v0x565461ce60b0_0 .var "a", 7 0;
v0x565461d092b0_0 .var "b", 7 0;
; Variable sum is vec4 return value of scope S_0x565461cbe6c0
TD_sum_of_twobit.sum ;
    %load/vec4 v0x565461ce60b0_0;
    %load/vec4 v0x565461d092b0_0;
    %add;
    %ret/vec4 0, 0, 8;  Assign to sum (store_vec4_to_lval)
    %end;
    .scope S_0x565461cbe510;
T_1 ;
    %delay 2, 0;
    %load/vec4 v0x565461d09450_0;
    %inv;
    %store/vec4 v0x565461d09450_0, 0, 8;
    %delay 3, 0;
    %load/vec4 v0x565461d09550_0;
    %inv;
    %store/vec4 v0x565461d09550_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_0x565461cbe510;
T_2 ;
    %wait E_0x565461cbec20;
    %load/vec4 v0x565461d09450_0;
    %load/vec4 v0x565461d09550_0;
    %store/vec4 v0x565461d092b0_0, 0, 8;
    %store/vec4 v0x565461ce60b0_0, 0, 8;
    %callf/vec4 TD_sum_of_twobit.sum, S_0x565461cbe6c0;
    %store/vec4 v0x565461d09630_0, 0, 8;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x565461cbe510;
T_3 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x565461d09450_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x565461d09550_0, 0, 8;
    %vpi_call 2 22 "$monitor", "Time=%0t a=%0d d=%0d c=%0d", $time, v0x565461d09450_0, v0x565461d09550_0, v0x565461d09630_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "sum_of_two.v";
