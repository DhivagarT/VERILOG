#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61043d414530 .scope module, "difference" "difference" 2 1;
 .timescale 0 0;
v0x61043d45e4b0_0 .var/i "a", 31 0;
v0x61043d45e5b0_0 .var/i "b", 31 0;
v0x61043d45e690_0 .var/i "c", 31 0;
S_0x61043d4146c0 .scope function.vec4.u32, "diff" "diff" 2 5, 2 5 0, S_0x61043d414530;
 .timescale 0 0;
; Variable diff is vec4 return value of scope S_0x61043d4146c0
v0x61043d45e310_0 .var/i "x", 31 0;
v0x61043d45e3f0_0 .var/i "y", 31 0;
TD_difference.diff ;
    %load/vec4 v0x61043d45e310_0;
    %load/vec4 v0x61043d45e3f0_0;
    %sub;
    %ret/vec4 0, 0, 32;  Assign to diff (store_vec4_to_lval)
    %end;
    .scope S_0x61043d414530;
T_1 ;
    %vpi_func 2 14 "$urandom" 32 {0 0 0};
    %pushi/vec4 15, 0, 32;
    %mod;
    %store/vec4 v0x61043d45e4b0_0, 0, 32;
    %vpi_func 2 15 "$urandom" 32 {0 0 0};
    %pushi/vec4 15, 0, 32;
    %mod;
    %store/vec4 v0x61043d45e5b0_0, 0, 32;
    %load/vec4 v0x61043d45e4b0_0;
    %load/vec4 v0x61043d45e5b0_0;
    %store/vec4 v0x61043d45e3f0_0, 0, 32;
    %store/vec4 v0x61043d45e310_0, 0, 32;
    %callf/vec4 TD_difference.diff, S_0x61043d4146c0;
    %store/vec4 v0x61043d45e690_0, 0, 32;
    %vpi_call 2 17 "$display", "Time=%0t a=%0d b=%0d c=%0d", $time, v0x61043d45e4b0_0, v0x61043d45e5b0_0, v0x61043d45e690_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "difference_of_two.v";
