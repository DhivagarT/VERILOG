    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x5f6018c47ee0_0, 0, 32;
    %jmp T_0.3;
T_0.2 ;
    %retload/vec4 0; Load count_tz (draw_signal_vec4)
    %addi 1, 0, 4;
    %ret/vec4 0, 0, 4;  Assign to count_tz (store_vec4_to_lval)
T_0.3 ;
    %load/vec4 v0x5f6018c47ee0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5f6018c47ee0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
