#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55e15414b570 .scope module, "multiply_task" "multiply_task" 2 1;
 .timescale 0 0;
v0x55e1541943a0_0 .var "a", 7 0;
v0x55e1541944a0_0 .var "b", 7 0;
S_0x55e15414b700 .scope task, "multiply" "multiply" 2 4, 2 4 0, S_0x55e15414b570;
 .timescale 0 0;
v0x55e154171910_0 .var "x", 7 0;
v0x55e1541942c0_0 .var "y", 7 0;
TD_multiply_task.multiply ;
    %load/vec4 v0x55e154171910_0;
    %load/vec4 v0x55e1541942c0_0;
    %mul;
    %vpi_call 2 7 "$display", "%0d * %0d = %0d", v0x55e154171910_0, v0x55e1541942c0_0, S<0,vec4,u8> {1 0 0};
    %end;
    .scope S_0x55e15414b570;
T_1 ;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x55e1541943a0_0, 0, 8;
    %pushi/vec4 7, 0, 8;
    %store/vec4 v0x55e1541944a0_0, 0, 8;
    %load/vec4 v0x55e1541943a0_0;
    %store/vec4 v0x55e154171910_0, 0, 8;
    %load/vec4 v0x55e1541944a0_0;
    %store/vec4 v0x55e1541942c0_0, 0, 8;
    %fork TD_multiply_task.multiply, S_0x55e15414b700;
    %join;
    %pushi/vec4 12, 0, 8;
    %store/vec4 v0x55e1541943a0_0, 0, 8;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x55e1541944a0_0, 0, 8;
    %load/vec4 v0x55e1541943a0_0;
    %store/vec4 v0x55e154171910_0, 0, 8;
    %load/vec4 v0x55e1541944a0_0;
    %store/vec4 v0x55e1541942c0_0, 0, 8;
    %fork TD_multiply_task.multiply, S_0x55e15414b700;
    %join;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
