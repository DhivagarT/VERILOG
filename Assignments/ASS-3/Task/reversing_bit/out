:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d2271682510 .scope module, "reverse_bits" "reverse_bits" 2 1;
 .timescale 0 0;
v0x5d22716cd780_0 .var "in", 7 0;
v0x5d22716cd880_0 .var "out", 7 0;
S_0x5d22716826a0 .scope task, "reverse" "reverse" 2 6, 2 6 0, S_0x5d2271682510;
 .timescale 0 0;
v0x5d22716a9d20_0 .var "data_in", 7 0;
v0x5d22716cd5e0_0 .var "data_out", 7 0;
v0x5d22716cd6c0_0 .var/i "j", 31 0;
TD_reverse_bits.reverse ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5d22716cd5e0_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5d22716cd6c0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5d22716cd6c0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5d22716a9d20_0;
    %pushi/vec4 7, 0, 32;
    %load/vec4 v0x5d22716cd6c0_0;
    %sub;
    %part/s 1;
    %ix/getv/s 4, v0x5d22716cd6c0_0;
    %store/vec4 v0x5d22716cd5e0_0, 4, 1;
    %load/vec4 v0x5d22716cd6c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5d22716cd6c0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x5d2271682510;
T_1 ;
    %pushi/vec4 178, 0, 8;
    %store/vec4 v0x5d22716cd780_0, 0, 8;
    %load/vec4 v0x5d22716cd780_0;
    %store/vec4 v0x5d22716a9d20_0, 0, 8;
    %fork TD_reverse_bits.reverse, S_0x5d22716826a0;
    %join;
    %load/vec4 v0x5d22716cd5e0_0;
    %store/vec4 v0x5d22716cd880_0, 0, 8;
    %vpi_call 2 20 "$display", "Input = %b, Reversed = %b", v0x5d22716cd780_0, v0x5d22716cd880_0 {0 0 0};
    %pushi/vec4 204, 0, 8;
    %store/vec4 v0x5d22716cd780_0, 0, 8;
    %load/vec4 v0x5d22716cd780_0;
    %store/vec4 v0x5d22716a9d20_0, 0, 8;
    %fork TD_reverse_bits.reverse, S_0x5d22716826a0;
    %join;
    %load/vec4 v0x5d22716cd5e0_0;
    %store/vec4 v0x5d22716cd880_0, 0, 8;
    %vpi_call 2 24 "$display", "Input = %b, Reversed = %b", v0x5d22716cd780_0, v0x5d22716cd880_0 {0 0 0};
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
