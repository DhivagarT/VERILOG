#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x62cda8369540 .scope module, "square_of_8bit" "square_of_8bit" 2 1;
 .timescale 0 0;
v0x62cda8369860_0 .var "a", 7 0;
v0x62cda83b3570_0 .var "c", 7 0;
v0x62cda83b3650_0 .var/i "i", 31 0;
S_0x62cda83696d0 .scope task, "square" "square" 2 10, 2 10 0, S_0x62cda8369540;
 .timescale 0 0;
TD_square_of_8bit.square ;
    %load/vec4 v0x62cda8369860_0;
    %pushi/vec4 2, 0, 32;
    %pow/s;
    %store/vec4 v0x62cda83b3570_0, 0, 8;
    %end;
    .scope S_0x62cda8369540;
T_1 ;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x62cda8369860_0, 0, 8;
    %end;
    .thread T_1;
    .scope S_0x62cda8369540;
T_2 ;
    %vpi_call 2 18 "$monitor", "Time=%0t a=%0d c=%0d", $time, v0x62cda8369860_0, v0x62cda83b3570_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x62cda83b3650_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x62cda83b3650_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_2.1, 5;
    %vpi_func 2 20 "$urandom" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod;
    %pad/u 8;
    %store/vec4 v0x62cda8369860_0, 0, 8;
    %fork TD_square_of_8bit.square, S_0x62cda83696d0;
    %join;
    %delay 3, 0;
    %load/vec4 v0x62cda83b3650_0;
    %addi 1, 0, 32;
    %store/vec4 v0x62cda83b3650_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "square_of_8b_input.v";
