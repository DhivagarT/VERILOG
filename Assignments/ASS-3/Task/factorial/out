#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5a4bf0520560 .scope module, "factorial_task" "factorial_task" 2 1;
 .timescale 0 0;
v0x5a4bf056a140_0 .var "fact", 31 0;
v0x5a4bf056a240_0 .var/i "i", 31 0;
v0x5a4bf056a320_0 .var "n", 7 0;
S_0x5a4bf05206f0 .scope task, "calc_factorial" "calc_factorial" 2 6, 2 6 0, S_0x5a4bf0520560;
 .timescale 0 0;
v0x5a4bf0547170_0 .var "num", 7 0;
v0x5a4bf056a060_0 .var "result", 31 0;
TD_factorial_task.calc_factorial ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x5a4bf056a060_0, 0, 32;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x5a4bf056a240_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5a4bf056a240_0;
    %load/vec4 v0x5a4bf0547170_0;
    %pad/u 32;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5a4bf056a060_0;
    %load/vec4 v0x5a4bf056a240_0;
    %mul;
    %store/vec4 v0x5a4bf056a060_0, 0, 32;
    %load/vec4 v0x5a4bf056a240_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5a4bf056a240_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x5a4bf0520560;
T_1 ;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x5a4bf056a320_0, 0, 8;
    %load/vec4 v0x5a4bf056a320_0;
    %store/vec4 v0x5a4bf0547170_0, 0, 8;
    %fork TD_factorial_task.calc_factorial, S_0x5a4bf05206f0;
    %join;
    %load/vec4 v0x5a4bf056a060_0;
    %store/vec4 v0x5a4bf056a140_0, 0, 32;
    %vpi_call 2 19 "$display", "Factorial of %0d is %0d", v0x5a4bf056a320_0, v0x5a4bf056a140_0 {0 0 0};
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "factorial.v";
