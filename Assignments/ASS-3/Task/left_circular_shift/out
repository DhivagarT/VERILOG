#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56d2b28d4590 .scope module, "left_circular_shift" "left_circular_shift" 2 1;
 .timescale 0 0;
v0x56d2b291e0b0_0 .var "data", 7 0;
S_0x56d2b28d4720 .scope task, "lshift" "lshift" 2 4, 2 4 0, S_0x56d2b28d4590;
 .timescale 0 0;
v0x56d2b28fb420_0 .var "in", 7 0;
v0x56d2b291dfd0_0 .var "out", 7 0;
TD_left_circular_shift.lshift ;
    %load/vec4 v0x56d2b28fb420_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x56d2b28fb420_0;
    %parti/s 1, 7, 4;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x56d2b291dfd0_0, 0, 8;
    %end;
    .scope S_0x56d2b28d4590;
T_1 ;
    %pushi/vec4 178, 0, 8;
    %store/vec4 v0x56d2b291e0b0_0, 0, 8;
    %vpi_call 2 14 "$display", "Original = %b", v0x56d2b291e0b0_0 {0 0 0};
    %load/vec4 v0x56d2b291e0b0_0;
    %store/vec4 v0x56d2b28fb420_0, 0, 8;
    %fork TD_left_circular_shift.lshift, S_0x56d2b28d4720;
    %join;
    %load/vec4 v0x56d2b291dfd0_0;
    %store/vec4 v0x56d2b291e0b0_0, 0, 8;
    %vpi_call 2 16 "$display", "After 1 shift = %b", v0x56d2b291e0b0_0 {0 0 0};
    %load/vec4 v0x56d2b291e0b0_0;
    %store/vec4 v0x56d2b28fb420_0, 0, 8;
    %fork TD_left_circular_shift.lshift, S_0x56d2b28d4720;
    %join;
    %load/vec4 v0x56d2b291dfd0_0;
    %store/vec4 v0x56d2b291e0b0_0, 0, 8;
    %vpi_call 2 18 "$display", "After 2 shifts = %b", v0x56d2b291e0b0_0 {0 0 0};
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
