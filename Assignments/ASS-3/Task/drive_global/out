#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x586f611c9490 .scope module, "drive_global_task" "drive_global_task" 2 1;
 .timescale 0 0;
v0x586f61212430_0 .var "global_var", 7 0;
S_0x586f611c9620 .scope task, "set_global" "set_global" 2 4, 2 4 0, S_0x586f611c9490;
 .timescale 0 0;
v0x586f611c9800_0 .var "value", 7 0;
TD_drive_global_task.set_global ;
    %load/vec4 v0x586f611c9800_0;
    %store/vec4 v0x586f61212430_0, 0, 8;
    %end;
    .scope S_0x586f611c9490;
T_1 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x586f61212430_0, 0, 8;
    %vpi_call 2 13 "$display", "Initial global_var = %0d", v0x586f61212430_0 {0 0 0};
    %pushi/vec4 42, 0, 8;
    %store/vec4 v0x586f611c9800_0, 0, 8;
    %fork TD_drive_global_task.set_global, S_0x586f611c9620;
    %join;
    %vpi_call 2 16 "$display", "After task call, global_var = %0d", v0x586f61212430_0 {0 0 0};
    %pushi/vec4 100, 0, 8;
    %store/vec4 v0x586f611c9800_0, 0, 8;
    %fork TD_drive_global_task.set_global, S_0x586f611c9620;
    %join;
    %vpi_call 2 19 "$display", "After another task call, global_var = %0d", v0x586f61212430_0 {0 0 0};
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "drive_global.v";
