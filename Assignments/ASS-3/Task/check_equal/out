S_0x5fd1ec832530 .scope module, "check_equal_task" "check_equal_task" 2 1;
 .timescale 0 0;
v0x5fd1ec87d2e0_0 .var "a", 7 0;
v0x5fd1ec87d3e0_0 .var "b", 7 0;
v0x5fd1ec87d4c0_0 .var "flag", 0 0;
S_0x5fd1ec8326c0 .scope task, "check_equal" "check_equal" 2 5, 2 5 0, S_0x5fd1ec832530;
 .timescale 0 0;
v0x5fd1ec859bb0_0 .var "f", 0 0;
v0x5fd1ec87d140_0 .var "x", 7 0;
v0x5fd1ec87d220_0 .var "y", 7 0;
TD_check_equal_task.check_equal ;
    %load/vec4 v0x5fd1ec87d140_0;
    %load/vec4 v0x5fd1ec87d220_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5fd1ec859bb0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5fd1ec859bb0_0, 0, 1;
T_0.1 ;
    %end;
    .scope S_0x5fd1ec832530;
T_1 ;
    %pushi/vec4 12, 0, 8;
    %store/vec4 v0x5fd1ec87d2e0_0, 0, 8;
    %pushi/vec4 12, 0, 8;
    %store/vec4 v0x5fd1ec87d3e0_0, 0, 8;
    %load/vec4 v0x5fd1ec87d2e0_0;
    %store/vec4 v0x5fd1ec87d140_0, 0, 8;
    %load/vec4 v0x5fd1ec87d3e0_0;
    %store/vec4 v0x5fd1ec87d220_0, 0, 8;
    %fork TD_check_equal_task.check_equal, S_0x5fd1ec8326c0;
    %join;
    %load/vec4 v0x5fd1ec859bb0_0;
    %store/vec4 v0x5fd1ec87d4c0_0, 0, 1;
    %vpi_call 2 20 "$display", "a = %0d, b = %0d, Equal? %0d", v0x5fd1ec87d2e0_0, v0x5fd1ec87d3e0_0, v0x5fd1ec87d4c0_0 {0 0 0};
    %pushi/vec4 7, 0, 8;
    %store/vec4 v0x5fd1ec87d2e0_0, 0, 8;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x5fd1ec87d3e0_0, 0, 8;
    %load/vec4 v0x5fd1ec87d2e0_0;
    %store/vec4 v0x5fd1ec87d140_0, 0, 8;
    %load/vec4 v0x5fd1ec87d3e0_0;
    %store/vec4 v0x5fd1ec87d220_0, 0, 8;
