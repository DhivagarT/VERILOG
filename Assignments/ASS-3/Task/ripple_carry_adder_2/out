#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c257fc64490 .scope module, "serial_add_task" "serial_add_task" 2 1;
 .timescale 0 0;
v0x5c257fcb2a50_0 .var "a", 3 0;
v0x5c257fcb2b50_0 .var "b", 3 0;
v0x5c257fcb2c30_0 .var "sum", 4 0;
S_0x5c257fc64620 .scope task, "serial_add" "serial_add" 2 6, 2 6 0, S_0x5c257fc64490;
 .timescale 0 0;
v0x5c257fc64800_0 .var "carry", 0 0;
v0x5c257fcb26a0_0 .var/i "j", 31 0;
v0x5c257fcb2780_0 .var "result", 4 0;
v0x5c257fcb2840_0 .var "x", 3 0;
v0x5c257fcb2920_0 .var "y", 3 0;
TD_serial_add_task.serial_add ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x5c257fcb2780_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c257fc64800_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c257fcb26a0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5c257fcb26a0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5c257fcb2840_0;
    %load/vec4 v0x5c257fcb26a0_0;
    %part/s 1;
    %load/vec4 v0x5c257fcb2920_0;
    %load/vec4 v0x5c257fcb26a0_0;
    %part/s 1;
    %xor;
    %load/vec4 v0x5c257fc64800_0;
    %xor;
    %ix/getv/s 4, v0x5c257fcb26a0_0;
    %store/vec4 v0x5c257fcb2780_0, 4, 1;
    %load/vec4 v0x5c257fcb2840_0;
    %load/vec4 v0x5c257fcb26a0_0;
    %part/s 1;
    %load/vec4 v0x5c257fcb2920_0;
    %load/vec4 v0x5c257fcb26a0_0;
    %part/s 1;
    %and;
    %load/vec4 v0x5c257fcb2840_0;
    %load/vec4 v0x5c257fcb26a0_0;
    %part/s 1;
    %load/vec4 v0x5c257fc64800_0;
    %and;
    %or;
    %load/vec4 v0x5c257fcb2920_0;
    %load/vec4 v0x5c257fcb26a0_0;
    %part/s 1;
    %load/vec4 v0x5c257fc64800_0;
    %and;
    %or;
    %store/vec4 v0x5c257fc64800_0, 0, 1;
    %load/vec4 v0x5c257fcb26a0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c257fcb26a0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x5c257fc64800_0;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5c257fcb2780_0, 4, 1;
    %end;
    .scope S_0x5c257fc64490;
T_1 ;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x5c257fcb2a50_0, 0, 4;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x5c257fcb2b50_0, 0, 4;
    %load/vec4 v0x5c257fcb2a50_0;
    %store/vec4 v0x5c257fcb2840_0, 0, 4;
    %load/vec4 v0x5c257fcb2b50_0;
    %store/vec4 v0x5c257fcb2920_0, 0, 4;
    %fork TD_serial_add_task.serial_add, S_0x5c257fc64620;
    %join;
    %load/vec4 v0x5c257fcb2780_0;
    %store/vec4 v0x5c257fcb2c30_0, 0, 5;
    %vpi_call 2 26 "$display", "a = %b, b = %b, sum = %b", v0x5c257fcb2a50_0, v0x5c257fcb2b50_0, v0x5c257fcb2c30_0 {0 0 0};
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x5c257fcb2a50_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5c257fcb2b50_0, 0, 4;
    %load/vec4 v0x5c257fcb2a50_0;
    %store/vec4 v0x5c257fcb2840_0, 0, 4;
    %load/vec4 v0x5c257fcb2b50_0;
    %store/vec4 v0x5c257fcb2920_0, 0, 4;
    %fork TD_serial_add_task.serial_add, S_0x5c257fc64620;
    %join;
    %load/vec4 v0x5c257fcb2780_0;
    %store/vec4 v0x5c257fcb2c30_0, 0, 5;
    %vpi_call 2 31 "$display", "a = %b, b = %b, sum = %b", v0x5c257fcb2a50_0, v0x5c257fcb2b50_0, v0x5c257fcb2c30_0 {0 0 0};
    %vpi_call 2 33 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
