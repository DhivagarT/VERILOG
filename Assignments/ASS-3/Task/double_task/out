#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5aecf8e961c0 .scope module, "task_call_demo" "task_call_demo" 2 1;
 .timescale 0 0;
v0x5aecf8ebb630_0 .var "A", 7 0;
v0x5aecf8ebb730_0 .var "B", 7 0;
v0x5aecf8ebb810_0 .var "result", 7 0;
S_0x5aecf8e6f4f0 .scope task, "add_task" "add_task" 2 5, 2 5 0, S_0x5aecf8e961c0;
 .timescale 0 0;
v0x5aecf8e6f6d0_0 .var "sum", 7 0;
v0x5aecf8ebaf00_0 .var "x", 7 0;
v0x5aecf8ebafe0_0 .var "y", 7 0;
TD_task_call_demo.add_task ;
    %load/vec4 v0x5aecf8ebaf00_0;
    %load/vec4 v0x5aecf8ebafe0_0;
    %add;
    %store/vec4 v0x5aecf8e6f6d0_0, 0, 8;
    %end;
S_0x5aecf8ebb0a0 .scope task, "double_add_task" "double_add_task" 2 13, 2 13 0, S_0x5aecf8e961c0;
 .timescale 0 0;
v0x5aecf8ebb2a0_0 .var "p", 7 0;
v0x5aecf8ebb380_0 .var "q", 7 0;
v0x5aecf8ebb460_0 .var "res", 7 0;
v0x5aecf8ebb550_0 .var "temp", 7 0;
TD_task_call_demo.double_add_task ;
    %load/vec4 v0x5aecf8ebb2a0_0;
    %store/vec4 v0x5aecf8ebaf00_0, 0, 8;
    %load/vec4 v0x5aecf8ebb380_0;
    %store/vec4 v0x5aecf8ebafe0_0, 0, 8;
    %fork TD_task_call_demo.add_task, S_0x5aecf8e6f4f0;
    %join;
    %load/vec4 v0x5aecf8e6f6d0_0;
    %store/vec4 v0x5aecf8ebb550_0, 0, 8;
    %load/vec4 v0x5aecf8ebb550_0;
    %store/vec4 v0x5aecf8ebaf00_0, 0, 8;
    %load/vec4 v0x5aecf8ebb550_0;
    %store/vec4 v0x5aecf8ebafe0_0, 0, 8;
    %fork TD_task_call_demo.add_task, S_0x5aecf8e6f4f0;
    %join;
    %load/vec4 v0x5aecf8e6f6d0_0;
    %store/vec4 v0x5aecf8ebb460_0, 0, 8;
    %end;
    .scope S_0x5aecf8e961c0;
T_2 ;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x5aecf8ebb630_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x5aecf8ebb730_0, 0, 8;
    %load/vec4 v0x5aecf8ebb630_0;
    %store/vec4 v0x5aecf8ebb2a0_0, 0, 8;
    %load/vec4 v0x5aecf8ebb730_0;
    %store/vec4 v0x5aecf8ebb380_0, 0, 8;
    %fork TD_task_call_demo.double_add_task, S_0x5aecf8ebb0a0;
    %join;
    %load/vec4 v0x5aecf8ebb460_0;
    %store/vec4 v0x5aecf8ebb810_0, 0, 8;
    %vpi_call 2 27 "$display", "Result = %0d", v0x5aecf8ebb810_0 {0 0 0};
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
