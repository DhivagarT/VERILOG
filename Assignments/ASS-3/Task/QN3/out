:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x58c1843e8510 .scope module, "two_task" "two_task" 2 1;
 .timescale 0 0;
v0x58c184410830_0 .var "a", 0 0;
v0x58c184434e20_0 .var "b", 0 0;
v0x58c184434ee0_0 .var "c", 0 0;
v0x58c184434f80_0 .var/i "i", 31 0;
S_0x58c1843e86a0 .scope task, "and_g" "and_g" 2 20, 2 20 0, S_0x58c1843e8510;
 .timescale 0 0;
TD_two_task.and_g ;
    %delay 4, 0;
    %load/vec4 v0x58c184410830_0;
    %load/vec4 v0x58c184434e20_0;
    %and;
    %store/vec4 v0x58c184434ee0_0, 0, 1;
    %end;
S_0x58c184410650 .scope task, "or_g" "or_g" 2 27, 2 27 0, S_0x58c1843e8510;
 .timescale 0 0;
TD_two_task.or_g ;
    %delay 6, 0;
    %load/vec4 v0x58c184410830_0;
    %load/vec4 v0x58c184434e20_0;
    %or;
    %store/vec4 v0x58c184434ee0_0, 0, 1;
    %end;
    .scope S_0x58c1843e8510;
T_2 ;
    %vpi_func 2 7 "$urandom" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %store/vec4 v0x58c184410830_0, 0, 1;
    %vpi_func 2 8 "$urandom" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %store/vec4 v0x58c184434e20_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x58c1843e8510;
T_3 ;
T_3.0 ;
    %delay 1, 0;
    %load/vec4 v0x58c184410830_0;
    %inv;
    %store/vec4 v0x58c184410830_0, 0, 1;
    %delay 2, 0;
    %load/vec4 v0x58c184434e20_0;
    %inv;
    %store/vec4 v0x58c184434e20_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x58c1843e8510;
T_4 ;
    %vpi_call 2 35 "$monitor", "Time=%0t a=%0d b=%0d c=%0d", $time, v0x58c184410830_0, v0x58c184434e20_0, v0x58c184434ee0_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x58c184434f80_0, 0, 32;
T_4.0 ;
    %load/vec4 v0x58c184434f80_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_4.1, 5;
    %fork TD_two_task.and_g, S_0x58c1843e86a0;
    %join;
    %fork TD_two_task.or_g, S_0x58c184410650;
    %join;
    %delay 1, 0;
    %load/vec4 v0x58c184434f80_0;
    %addi 1, 0, 32;
    %store/vec4 v0x58c184434f80_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %vpi_call 2 41 "$finish" {0 0 0};
    %end;
    .thread T_4;
