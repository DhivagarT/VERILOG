#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ad02d3d0510 .scope module, "addition_of_8_bits" "addition_of_8_bits" 2 1;
 .timescale 0 0;
v0x5ad02d3f7b80_0 .var "a", 7 0;
v0x5ad02d41a900_0 .var "b", 7 0;
v0x5ad02d41a9e0_0 .var "c", 8 0;
E_0x5ad02d3d0eb0 .event anyedge, v0x5ad02d3f7b80_0, v0x5ad02d41a900_0;
S_0x5ad02d3d06c0 .scope task, "add" "add" 2 4, 2 4 0, S_0x5ad02d3d0510;
 .timescale 0 0;
TD_addition_of_8_bits.add ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5ad02d3f7b80_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x5ad02d41a900_0, 0, 8;
    %delay 2, 0;
    %load/vec4 v0x5ad02d3f7b80_0;
    %inv;
    %store/vec4 v0x5ad02d3f7b80_0, 0, 8;
    %delay 3, 0;
    %load/vec4 v0x5ad02d41a900_0;
    %inv;
    %store/vec4 v0x5ad02d41a900_0, 0, 8;
    %end;
    .scope S_0x5ad02d3d0510;
T_1 ;
    %wait E_0x5ad02d3d0eb0;
    %load/vec4 v0x5ad02d3f7b80_0;
    %pad/u 9;
    %load/vec4 v0x5ad02d41a900_0;
    %pad/u 9;
    %add;
    %store/vec4 v0x5ad02d41a9e0_0, 0, 9;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5ad02d3d0510;
T_2 ;
    %vpi_call 2 18 "$monitor", "Time=%0t  a=%0h  b=%0h  c=%0h ", $time, v0x5ad02d3f7b80_0, v0x5ad02d41a900_0, v0x5ad02d41a9e0_0 {0 0 0};
    %pushi/vec4 5, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %fork TD_addition_of_8_bits.add, S_0x5ad02d3d06c0;
    %join;
    %delay 5, 0;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "QN1.v";
