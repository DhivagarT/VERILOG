#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59a70c369540 .scope module, "toggling_for_10" "toggling_for_10" 2 1;
 .timescale 0 0;
v0x59a70c38ff50_0 .var "a", 0 0;
v0x59a70c3b1f70_0 .var "c", 0 0;
S_0x59a70c3696d0 .scope task, "toggle" "toggle" 2 6, 2 6 0, S_0x59a70c369540;
 .timescale 0 0;
TD_toggling_for_10.toggle ;
    %delay 10, 0;
    %load/vec4 v0x59a70c38ff50_0;
    %inv;
    %store/vec4 v0x59a70c3b1f70_0, 0, 1;
    %end;
    .scope S_0x59a70c369540;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x59a70c38ff50_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x59a70c369540;
T_2 ;
    %vpi_call 2 12 "$monitor", "Time=%0t a=%0d c=%0d", $time, v0x59a70c38ff50_0, v0x59a70c3b1f70_0 {0 0 0};
    %delay 11, 0;
    %vpi_func 2 13 "$urandom" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %store/vec4 v0x59a70c38ff50_0, 0, 1;
    %fork TD_toggling_for_10.toggle, S_0x59a70c3696d0;
    %join;
    %delay 5, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "toggling.v";
