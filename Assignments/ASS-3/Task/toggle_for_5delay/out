#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5972f0c0d590 .scope module, "toggle_clock" "toggle_clock" 2 1;
 .timescale 0 0;
v0x5972f0c55d40_0 .var "clk", 0 0;
S_0x5972f0c0d720 .scope task, "toggle_10_times" "toggle_10_times" 2 4, 2 4 0, S_0x5972f0c0d590;
 .timescale 0 0;
v0x5972f0c339e0_0 .var/i "i", 31 0;
TD_toggle_clock.toggle_10_times ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5972f0c339e0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5972f0c339e0_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5972f0c55d40_0;
    %inv;
    %store/vec4 v0x5972f0c55d40_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x5972f0c339e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5972f0c339e0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x5972f0c0d590;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5972f0c55d40_0, 0, 1;
    %fork TD_toggle_clock.toggle_10_times, S_0x5972f0c0d720;
    %join;
    %vpi_call 2 17 "$display", "Final clock state = %0d", v0x5972f0c55d40_0 {0 0 0};
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "toggle_for_5delay.v";
