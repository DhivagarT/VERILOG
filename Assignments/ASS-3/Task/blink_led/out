#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5679abaad5c0 .scope module, "blink_led" "blink_led" 2 1;
 .timescale 0 0;
v0x5679abaf6060_0 .var "led", 0 0;
S_0x5679abaad750 .scope task, "blink" "blink" 2 4, 2 4 0, S_0x5679abaad5c0;
 .timescale 0 0;
v0x5679abad3e50_0 .var "delay_time", 31 0;
TD_blink_led.blink ;
    %load/vec4 v0x5679abaf6060_0;
    %inv;
    %store/vec4 v0x5679abaf6060_0, 0, 1;
    %load/vec4 v0x5679abad3e50_0;
    %pad/u 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %end;
    .scope S_0x5679abaad5c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5679abaf6060_0, 0, 1;
    %pushi/vec4 5, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x5679abad3e50_0, 0, 32;
    %fork TD_blink_led.blink, S_0x5679abaad750;
    %join;
    %vpi_call 2 16 "$display", "Time=%0t LED=%0d", $time, v0x5679abaf6060_0 {0 0 0};
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "blink_led.v";
