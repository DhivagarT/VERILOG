#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d35e5ac24c0 .scope module, "average_task" "average_task" 2 1;
 .timescale 0 0;
v0x5d35e5ac2830_0 .var "a", 7 0;
v0x5d35e5b0c330_0 .var "avg", 7 0;
v0x5d35e5b0c410_0 .var "b", 7 0;
v0x5d35e5b0c4d0_0 .var "c", 7 0;
v0x5d35e5b0c5b0_0 .var "d", 7 0;
v0x5d35e5b0c6e0_0 .var "e", 7 0;
v0x5d35e5b0c7c0_0 .var "sum", 7 0;
S_0x5d35e5ac2650 .scope task, "calc_average" "calc_average" 2 6, 2 6 0, S_0x5d35e5ac24c0;
 .timescale 0 0;
TD_average_task.calc_average ;
    %load/vec4 v0x5d35e5ac2830_0;
    %load/vec4 v0x5d35e5b0c410_0;
    %add;
    %load/vec4 v0x5d35e5b0c4d0_0;
    %add;
    %load/vec4 v0x5d35e5b0c5b0_0;
    %add;
    %load/vec4 v0x5d35e5b0c6e0_0;
    %add;
    %store/vec4 v0x5d35e5b0c7c0_0, 0, 8;
    %load/vec4 v0x5d35e5b0c7c0_0;
    %pad/u 32;
    %pushi/vec4 5, 0, 32;
    %div;
    %pad/u 8;
    %store/vec4 v0x5d35e5b0c330_0, 0, 8;
    %end;
    .scope S_0x5d35e5ac24c0;
T_1 ;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x5d35e5ac2830_0, 0, 8;
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x5d35e5b0c410_0, 0, 8;
    %pushi/vec4 30, 0, 8;
    %store/vec4 v0x5d35e5b0c4d0_0, 0, 8;
    %pushi/vec4 40, 0, 8;
    %store/vec4 v0x5d35e5b0c5b0_0, 0, 8;
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0x5d35e5b0c6e0_0, 0, 8;
    %fork TD_average_task.calc_average, S_0x5d35e5ac2650;
    %join;
    %vpi_call 2 16 "$display", "Average = %0d", v0x5d35e5b0c330_0 {0 0 0};
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "average_of_array.v";
