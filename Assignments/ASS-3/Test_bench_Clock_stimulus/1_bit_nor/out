#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b14784be510 .scope module, "one_bit_nor" "one_bit_nor" 2 1;
 .timescale 0 0;
v0x5b14784be6c0_0 .var "a", 0 0;
v0x5b14785077e0_0 .var "b", 0 0;
v0x5b14785078a0_0 .var "y", 0 0;
E_0x5b14784be950 .event anyedge, v0x5b14784be6c0_0, v0x5b14785077e0_0;
    .scope S_0x5b14784be510;
T_0 ;
    %delay 1, 0;
    %load/vec4 v0x5b14784be6c0_0;
    %inv;
    %store/vec4 v0x5b14784be6c0_0, 0, 1;
    %load/vec4 v0x5b14785077e0_0;
    %inv;
    %store/vec4 v0x5b14785077e0_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b14784be510;
T_1 ;
    %wait E_0x5b14784be950;
    %load/vec4 v0x5b14784be6c0_0;
    %load/vec4 v0x5b14785077e0_0;
    %or;
    %inv;
    %store/vec4 v0x5b14785078a0_0, 0, 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5b14784be510;
T_2 ;
    %vpi_func 2 15 "$urandom" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %store/vec4 v0x5b14784be6c0_0, 0, 1;
    %vpi_func 2 16 "$urandom" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %store/vec4 v0x5b14785077e0_0, 0, 1;
    %vpi_call 2 17 "$monitor", "Time=%0t a=%0d b=%0d y=%0d", $time, v0x5b14784be6c0_0, v0x5b14785077e0_0, v0x5b14785078a0_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "1_bit_nor.v";
