#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6137db846510 .scope module, "strobe_demo" "strobe_demo" 2 1;
 .timescale 0 0;
v0x6137db8466c0_0 .var "a", 7 0;
v0x6137db88fe90_0 .var "b", 7 0;
v0x6137db88ff70_0 .var "c", 15 0;
E_0x6137db846a50 .event anyedge, v0x6137db8466c0_0, v0x6137db88fe90_0;
    .scope S_0x6137db846510;
T_0 ;
    %delay 2, 0;
    %load/vec4 v0x6137db8466c0_0;
    %addi 1, 0, 8;
    %store/vec4 v0x6137db8466c0_0, 0, 8;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6137db846510;
T_1 ;
    %delay 2, 0;
    %load/vec4 v0x6137db88fe90_0;
    %addi 1, 0, 8;
    %store/vec4 v0x6137db88fe90_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_0x6137db846510;
T_2 ;
    %wait E_0x6137db846a50;
    %load/vec4 v0x6137db8466c0_0;
    %pad/u 16;
    %load/vec4 v0x6137db88fe90_0;
    %pad/u 16;
    %mul;
    %store/vec4 v0x6137db88ff70_0, 0, 16;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x6137db846510;
T_3 ;
    %vpi_func 2 11 "$urandom" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod;
    %pad/u 8;
    %store/vec4 v0x6137db8466c0_0, 0, 8;
    %vpi_func 2 12 "$urandom" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod;
    %pad/u 8;
    %store/vec4 v0x6137db88fe90_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 14 "$strobe", "Time=%0t a=%0d b=%0d c=%0d", $time, v0x6137db8466c0_0, v0x6137db88fe90_0, v0x6137db88ff70_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "strobe_demo.v";
