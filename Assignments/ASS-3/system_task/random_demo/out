#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5859b2da9510 .scope module, "monitor_demo" "monitor_demo" 2 1;
 .timescale 0 0;
v0x5859b2da96c0_0 .var "a", 4 0;
v0x5859b2df31f0_0 .var "b", 4 0;
v0x5859b2df32d0_0 .var "c", 4 0;
E_0x5859b2da9a50 .event anyedge, v0x5859b2da96c0_0, v0x5859b2df31f0_0;
    .scope S_0x5859b2da9510;
T_0 ;
    %delay 2, 0;
    %load/vec4 v0x5859b2da96c0_0;
    %addi 1, 0, 5;
    %store/vec4 v0x5859b2da96c0_0, 0, 5;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5859b2da9510;
T_1 ;
    %delay 2, 0;
    %load/vec4 v0x5859b2df31f0_0;
    %addi 1, 0, 5;
    %store/vec4 v0x5859b2df31f0_0, 0, 5;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5859b2da9510;
T_2 ;
    %wait E_0x5859b2da9a50;
    %load/vec4 v0x5859b2da96c0_0;
    %load/vec4 v0x5859b2df31f0_0;
    %mod;
    %store/vec4 v0x5859b2df32d0_0, 0, 5;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x5859b2da9510;
T_3 ;
    %vpi_func 2 14 "$urandom_range" 32, 32'sb00000000000000000000000000000001, 32'sb00000000000000000000000000000111 {0 0 0};
    %pad/u 5;
    %store/vec4 v0x5859b2da96c0_0, 0, 5;
    %vpi_func 2 15 "$urandom_range" 32, 32'sb00000000000000000000000000000001, 32'sb00000000000000000000000000000111 {0 0 0};
    %pad/u 5;
    %store/vec4 v0x5859b2df31f0_0, 0, 5;
    %vpi_call 2 16 "$monitor", "Time=%0t a=%0d b=%0d c=%0d", $time, v0x5859b2da96c0_0, v0x5859b2df31f0_0, v0x5859b2df32d0_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "random_demo.v";
