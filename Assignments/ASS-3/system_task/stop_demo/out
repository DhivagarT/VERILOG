#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6204ae479540 .scope module, "stop_demo" "stop_demo" 2 1;
 .timescale 0 0;
v0x6204ae4796d0_0 .var "a", 3 0;
v0x6204ae4c3480_0 .var "b", 3 0;
    .scope S_0x6204ae479540;
T_0 ;
    %delay 2, 0;
    %load/vec4 v0x6204ae4796d0_0;
    %addi 1, 0, 4;
    %store/vec4 v0x6204ae4796d0_0, 0, 4;
    %delay 2, 0;
    %load/vec4 v0x6204ae4c3480_0;
    %addi 1, 0, 4;
    %store/vec4 v0x6204ae4c3480_0, 0, 4;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6204ae479540;
T_1 ;
    %vpi_func 2 11 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000001000 {0 0 0};
    %pad/u 4;
    %store/vec4 v0x6204ae4796d0_0, 0, 4;
    %vpi_func 2 12 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000001000 {0 0 0};
    %pad/u 4;
    %store/vec4 v0x6204ae4c3480_0, 0, 4;
    %vpi_call 2 13 "$display", "simulation start" {0 0 0};
    %delay 20, 0;
    %vpi_call 2 15 "$display", "Time=%0t a=%0d b=%0d", $time, v0x6204ae4796d0_0, v0x6204ae4c3480_0 {0 0 0};
    %vpi_call 2 16 "$stop" {0 0 0};
    %vpi_call 2 17 "$display", "press finish to resume" {0 0 0};
    %delay 20, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "stop_demo.v";
