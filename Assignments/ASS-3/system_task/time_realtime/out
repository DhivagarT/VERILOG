#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x61b5052c5510 .scope module, "time_realtime" "time_realtime" 2 3;
 .timescale -9 -12;
v0x61b5052c56c0_0 .var "a", 3 0;
v0x61b50530f280_0 .var "b", 3 0;
v0x61b50530f360_0 .var "c", 3 0;
E_0x61b5052c5a60 .event anyedge, v0x61b5052c56c0_0, v0x61b50530f280_0;
    .scope S_0x61b5052c5510;
T_0 ;
    %wait E_0x61b5052c5a60;
    %load/vec4 v0x61b5052c56c0_0;
    %load/vec4 v0x61b50530f280_0;
    %xnor;
    %store/vec4 v0x61b50530f360_0, 0, 4;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x61b5052c5510;
T_1 ;
    %vpi_func 2 10 "$urandom" 32 {0 0 0};
    %pushi/vec4 7, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x61b5052c56c0_0, 0, 4;
    %vpi_func 2 11 "$urandom" 32 {0 0 0};
    %pushi/vec4 7, 0, 32;
    %mod;
    %pad/u 4;
    %store/vec4 v0x61b50530f280_0, 0, 4;
    %end;
    .thread T_1;
    .scope S_0x61b5052c5510;
T_2 ;
    %delay 1300, 0;
    %load/vec4 v0x61b5052c56c0_0;
    %subi 1, 0, 4;
    %store/vec4 v0x61b5052c56c0_0, 0, 4;
    %delay 2100, 0;
    %load/vec4 v0x61b50530f280_0;
    %subi 1, 0, 4;
    %store/vec4 v0x61b50530f280_0, 0, 4;
    %jmp T_2;
    .thread T_2;
    .scope S_0x61b5052c5510;
T_3 ;
    %vpi_call 2 22 "$monitor", "Time=%0t realtime=%0f a=%0d b=%0d c=%0d", $time, $realtime, v0x61b5052c56c0_0, v0x61b50530f280_0, v0x61b50530f360_0 {0 0 0};
    %delay 30000, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "time_realtime.v";
