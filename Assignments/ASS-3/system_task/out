S_0x626aca31c540 .scope module, "sum" "sum" 2 1;
 .timescale 0 0;
v0x626aca31c6d0_0 .var "a", 0 0;
v0x626aca363790_0 .var "b", 0 0;
v0x626aca363850_0 .var "c", 0 0;
    .scope S_0x626aca31c540;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x626aca31c6d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x626aca363790_0, 0, 1;
    %load/vec4 v0x626aca31c6d0_0;
    %load/vec4 v0x626aca363790_0;
    %add;
    %store/vec4 v0x626aca363850_0, 0, 1;
    %vpi_call 2 8 "$display", "a=%0b b=%0b c=%0b", v0x626aca31c6d0_0, v0x626aca363790_0, v0x626aca363850_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_0;
