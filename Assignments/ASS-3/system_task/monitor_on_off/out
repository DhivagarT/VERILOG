#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x579f8a3b5540 .scope module, "moniotr_on_off" "moniotr_on_off" 2 1;
 .timescale 0 0;
v0x579f8a3b56d0_0 .var "a", 4 0;
v0x579f8a3ff950_0 .var "b", 4 0;
v0x579f8a3ffa30_0 .var "y", 4 0;
    .scope S_0x579f8a3b5540;
T_0 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x579f8a3b56d0_0, 0, 5;
    %pushi/vec4 1, 0, 5;
    %store/vec4 v0x579f8a3ff950_0, 0, 5;
    %end;
    .thread T_0;
    .scope S_0x579f8a3b5540;
T_1 ;
    %delay 2, 0;
    %load/vec4 v0x579f8a3b56d0_0;
    %addi 1, 0, 5;
    %store/vec4 v0x579f8a3b56d0_0, 0, 5;
    %load/vec4 v0x579f8a3ff950_0;
    %addi 1, 0, 5;
    %store/vec4 v0x579f8a3ff950_0, 0, 5;
    %jmp T_1;
    .thread T_1;
    .scope S_0x579f8a3b5540;
T_2 ;
    %vpi_func 2 16 "$urandom_range" 32, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000001111 {0 0 0};
    %pad/u 5;
    %store/vec4 v0x579f8a3b56d0_0, 0, 5;
    %vpi_func 2 17 "$urandom" 32 {0 0 0};
    %pushi/vec4 16, 0, 32;
    %mod;
    %pad/u 5;
    %store/vec4 v0x579f8a3ff950_0, 0, 5;
    %vpi_call 2 18 "$display", "time=%0t monitor off", $time {0 0 0};
    %vpi_call 2 19 "$monitoroff" {0 0 0};
    %delay 5, 0;
    %vpi_call 2 21 "$monitoron" {0 0 0};
    %load/vec4 v0x579f8a3b56d0_0;
    %load/vec4 v0x579f8a3ff950_0;
    %xor;
    %store/vec4 v0x579f8a3ffa30_0, 0, 5;
    %vpi_call 2 23 "$display", "time=%0t monitor on ", $time {0 0 0};
    %delay 20, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "monitor_on_off.v";
